@techreport{suit-23,
    number =    {draft-ietf-suit-manifest-23},
    type =      {Internet-Draft},
    institution =   {Internet Engineering Task Force},
    publisher = {Internet Engineering Task Force},
    url =       {https://datatracker.ietf.org/doc/draft-ietf-suit-manifest/23/},
        author =    {Brendan Moran and Hannes Tschofenig and Henk Birkholz and Koen Zandberg and Øyvind Rønningstad},
    title =     {{A Concise Binary Object Representation (CBOR)-based Serialization Format for the Software Updates for Internet of Things (SUIT) Manifest}},
    pagetotal = 96,
    year =      2023,
    month =     sep,
    day =       10,
    abstract =  {This specification describes the format of a manifest. A manifest is a bundle of metadata about code/data obtained by a recipient (chiefly the firmware for an IoT device), where to find the code/data, the devices to which it applies, and cryptographic information protecting the manifest. Software updates and Trusted Invocation both tend to use sequences of common operations, so the manifest encodes those sequences of operations, rather than declaring the metadata.}, note={\url{https://datatracker.ietf.org/doc/draft-ietf-suit-manifest/23/}}
}

